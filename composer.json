{
    "name": "dbi-playground",
    "description": "http://www.dbi-community.eu/",
    "keywords": ["future", "enterprise", "futureenterprise", "dbi"],
    "homepage": "http://www.dbi-community.eu/",
    "type": "project",
    "authors": [
        {
            "name": "Romanos Tsouroplis",
            "email": "rom-dim@hotmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/romdim/dbi-playground/issues?state=open",
        "source": "https://github.com/romdim/dbi-playground/"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "2.0.*",
        "yiisoft/yii2-bootstrap": "2.0.*",
        "yiisoft/yii2-swiftmailer": "2.0.*",
        "yiidoc/yii2-redactor": "~2.0",
        "kartik-v/yii2-slider": "1.3.*",
        "2amigos/yii2-chartjs-widget" : "~2.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "2.0.*",
        "yiisoft/yii2-debug": "2.0.*",
        "yiisoft/yii2-gii": "2.0.*",
        "yiisoft/yii2-faker": "2.0.*",
        "schmunk42/yii2-giiant": "0.5.*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
